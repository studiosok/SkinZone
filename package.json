{
  "name": "skinzone",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build/",
    "copy-files": "copyfiles -u 1 public/**/*.html public/**/*.css public/**/*.js build/public/",
    "copy-amplify": "copyfiles -u 1 public/**/*.html public/**/*.css public/**/*.js build/",
    "build": "yarn clean && tsc && yarn copy-files",
    "amplify-build": "yarn clean && tsc && yarn copy-amplify",
    "serve": "node build/server.js",
    "dev": "tsx ./server.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/studiosok/SkinZone.git"
  },
  "author": "Kristen Studioso",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/studiosok/SkinZone/issues"
  },
  "homepage": "https://github.com/studiosok/SkinZone#readme",
  "dependencies": {
    "express": "^5.1.0",
    "tsc": "^2.0.4",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "copyfiles": "^2.4.1",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.4"
  }
}
